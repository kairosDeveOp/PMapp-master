@model PMApp.Models.Building

@{
    ViewData["Title"] = "Details";
}

    <h1>@Model.Org_name </h1>

<div>
    <h4>Property details</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Unit_Count)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Unit_Count)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Street)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Street)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.City)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.City)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Zip_code)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Zip_code)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.State)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.State)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.TPID)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.TPID)
        </dd>
    </dl>
</div>
<div>
    <p>
        <input type="button" class="button" value="BACK TO LIST" onclick="history.go(-1); return false;">
    </p>
    <!-- <a asp-action="Index">Back to List</a> -->
</div>
